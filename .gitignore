# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log

.byebug_history

# Dependency directory
node_modules

# Build
build/app.*
build/vendor.*
build/style.css

# Logs
logs
*.log

# Generated
__generated__

# uploaded images
static/images/upload/*
!static/images/upload/README.md

/.yardoc/*
/doc/*
